# testing Lotka-Volterra interaction

# ODE
# base scenarios (density and frequency dynamics)
--module LV --model ODE --prey 0.667,-1.333 --predator 0,1 --deathrate 0,1 --init density 1,1 --timestop 100
--module LV --model ODE --prey 0.667,-1.333 --predator 0,1 --deathrate 0,1 --init frequency 1,1 --timestop 100

# SDE
# base scenarios (density and frequency dynamics)
--module LV --model SDE --prey 0.667,-1.333 --predator 0,1 --deathrate 0,1 --init density 1,1 --popsize 10000 --timestop 100
--module LV --model SDE --prey 0.667,-1.333 --predator 0,1 --deathrate 0,1 --init frequency 1,1 --popsize 10000 --timestop 100

# PDE not implemented

# IBS
# base scenarios (density and frequency dynamics)
--module LV --model IBS --prey 0.667,-1.333 --predator 0,1 --deathrate 0,1 --init density 1,1 --popsize 1000 --timestop 100
--module LV --model IBS --prey 0.667,-1.333 --predator 0,1 --deathrate 0,1 --init frequency 1,1 --popsize 1000 --timestop 100

# population sizes
--module LV --model IBS --prey 0.667,-1.333 --predator 0,1 --deathrate 0,1 --init frequency 1,1 --popsize 1000,10000 --timestop 100

# population update rates
--module LV --model IBS --prey 0.667,-1.333 --predator 0,1 --deathrate 0,1 --init frequency 1,1 --popsize 1000,10000 --speciesupdate size --timestop 100
--module LV --model IBS --prey 0.667,-1.333 --predator 0,1 --deathrate 0,1 --init frequency 1,1 --popsize 1000,10000 --speciesupdate rate --timestop 100
--module LV --model IBS --prey 0.667,-1.333 --predator 0,1 --deathrate 0,1 --init frequency 1,1 --popsize 1000,10000 --speciesupdate uniform --timestop 100
--module LV --model IBS --prey 0.667,-1.333 --predator 0,1 --deathrate 0,1 --init frequency 1,1 --popsize 1000,10000 --speciesupdate turns --timestop 100

# population structures
--module LV --model IBS --prey 0.667,-1.333 --predator 0,1 --deathrate 0,1 --init frequency 1,1  --popsize 1024 --geometry n --timestop 100
--module LV --model IBS --prey 0.667,-1.333 --predator 0,1 --deathrate 0,1 --init frequency 1,1  --popsize 1024 --geometry r4 --timestop 100
--module LV --model IBS --prey 0.667,-1.333 --predator 0,1 --deathrate 0,1 --init frequency 1,1  --popsize 1024 --geometry n:r4 --timestop 100
