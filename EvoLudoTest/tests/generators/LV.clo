# IBS
# base scenarios (density and frequency dynamics)
--module LV --model IBS --birthrate 0.667,0 --deathrate 0,1 --comprate 0,-1.333;1,0 --init density 1,1 --popsize 1000 --timestop 100
--module LV --model IBS --birthrate 0.667,0 --deathrate 0,1 --comprate 0,-1.333;1,0 --init frequency 1,1 --popsize 1000 --timestop 100

# population sizes
--module LV --model IBS --birthrate 0.667,0 --deathrate 0,1 --comprate 0,-1.333;1,0 --init frequency 1,1 --popsize 1000,10000 --timestop 100

# population update rates
--module LV --model IBS --birthrate 0.667,0 --deathrate 0,1 --comprate 0,-1.333;1,0 --init frequency 1,1 --popsize 1000,10000 --speciesupdate size --timestop 100
--module LV --model IBS --birthrate 0.667,0 --deathrate 0,1 --comprate 0,-1.333;1,0 --init frequency 1,1 --popsize 1000,10000 --speciesupdate rate --timestop 100
--module LV --model IBS --birthrate 0.667,0 --deathrate 0,1 --comprate 0,-1.333;1,0 --init frequency 1,1 --popsize 1000,10000 --speciesupdate uniform --timestop 100
